<odoo>

  <record model="ir.ui.view" id="document_part_report_search">
    <field name="name">Document Part Report Search</field>
    <field name="model">certificate_planer.document_part_report</field>
    <field name="arch" type="xml">
      <search>
        <field name="document"/>
        <field name="part"/>
      </search>
    </field>
  </record>

  <record model="ir.ui.view" id="document_part_report_list">
    <field name="name">Document Part Report List</field>
    <field name="model">certificate_planer.document_part_report</field>
    <field name="arch" type="xml">
      <tree limit="200" create="false" delete="false">
        <field name="document"/>
        <field name="part"/>
      </tree>
    </field>
  </record>

</odoo>
